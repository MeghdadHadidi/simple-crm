    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "restart": false,
            "sourceMaps": true,
            "localRoot": "${workspaceRoot}",
            "protocol": "inspector",
            "processId": "${command:PickProcess}",
            "remoteRoot": null
          },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\index.js"
        }
    ]
}